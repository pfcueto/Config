//$tab Pt
//============================================================================
PtAssessment:
SQL SELECT *
FROM CISReportingDB.dbo.PtAssessment
where year(storeTime)=$(vYearMax);
STORE PtAssessment INTO $(vG.ExtractPath)PtAssessment_$(vYearMax).qvd (qvd);
DROP TABLE PtAssessment;

